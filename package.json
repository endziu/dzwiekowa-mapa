    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.0",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4"
  }
