{
  "name": "dzwiekowa-mapa",
  "version": "3.0.0",
  "description": "dzwiękowa mapa wrocławia",
  "main": "./src/server/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "export NODE_ENV=production && webpack -p --config webpack.config.client.js && webpack -p --config webpack.config.server.js",
    "start": "export PORT=16661 && pm2 start dist/server.js --name=\"mapa\"",
    "deploy": "yarn build && yarn start",
    "restart": "pm2 restart mapa",
    "dev:server": "webpack --config webpack.config.server.js --watch",
    "dev:client": "webpack --config webpack.config.client.js --watch",
    "dev:start": "set PORT=16661 && nodemon dist/server.js --watch dist/server.js",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\" \"yarn dev:start\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/endziu/dzwiekowa-mapa.git"
  },
  "author": "Andrzej Koper <koper.andrzej@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/endziu/dzwiekowa-mapa/issues"
  },
  "homepage": "https://dzwiekowamapa.pl",
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.1",
    "express-favicon": "^2.0.0",
    "fs-readfile-promise": "^3.0.0",
    "helmet": "^3.5.0",
    "morgan": "^1.8.1",
    "node-fetch": "^1.6.3",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-gmaps": "^1.5.0",
    "react-image": "^1.0.1",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "source-map-support": "^0.4.14",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",    
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.0",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4"
  }
}
