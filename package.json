  },
  "homepage": "https://dzwiekowa-mapa.now.sh/",
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.1",
    "express-favicon": "^2.0.0",
    "fs-readfile-promise": "^3.0.0",
    "helmet": "^3.5.0",
    "morgan": "^1.8.1",
    "node-fetch": "^1.6.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-gmaps": "^1.5.0",
    "react-router": "^4.0.0",
